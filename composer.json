{
  "name": "rolice/speedy",
  "description": "Composer package for local courier service provider - Speedy.",
  "type": "library",
  "version": "1.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Lyubomir Gardev",
      "email": "rolice@intellisys.info"
    }
  ],
  "require": {
    "illuminate/support": "^5.2",
    "php": ">=5.6",
    "ext-soap": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "Rolice\\Speedy\\": "src",
      "Rolice\\Speedy\\Models\\": "src/Models/",
      "Rolice\\Speedy\\Commands\\": "src/Commands/",
      "Rolice\\Speedy\\Components\\": "src/Components/",
      "Rolice\\Speedy\\Exceptions\\": "src/Exceptions/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rolice\\Speedy\\Test\\": "tests/"
    }
  }
}
